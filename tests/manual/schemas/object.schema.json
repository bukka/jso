{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "minimum": 1
    },
    "name": {
      "type": "string",
      "minLength": 1
    },
    "active": {
      "type": "boolean"
    }
  },
  "required": ["id", "name"],
  "additionalProperties": false
}
